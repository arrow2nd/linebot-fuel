{
  "imports": {
    "express": "npm:express@4.18.2",
    "bot-sdk": "npm:@line/bot-sdk@7.5.2",
    "dayjs": "npm:dayjs@1.11.7",
    "dayjs/timezone": "npm:dayjs@1.11.7/plugin/timezone.js",
    "dayjs/utc": "npm:dayjs@1.11.7/plugin/utc.js",
    "http": "https://deno.land/std@0.186.0/http/mod.ts",
    "dotenv": "https://deno.land/x/dotenv@v3.2.2/load.ts"
  },
  "tasks": {
    "run": "deno run --allow-env --allow-net --allow-read server.ts",
    "docker:build": "docker image build -t linebot-fuel .",
    "docker:run": "docker run -it --env-file=./.env -p 8080:8080 linebot-fuel"
  },
  "lock": false
}
